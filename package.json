  "dependenciesMeta": {
    "esbuild": {
      "built": true
    },
    "puppeteer": {
      "built": true
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "puppeteer",
      "webdriver-manager"
    ],
    "overrides": {
      "@angular/build": "workspace:*"
    },
    "packageExtensions": {
      "grpc-gcp": {
        "peerDependencies": {
          "protobufjs": "*"
        }
      }
    }
  },
  "resolutions": {
    "typescript": "5.9.2"
  }
